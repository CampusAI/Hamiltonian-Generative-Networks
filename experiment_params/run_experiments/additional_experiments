# Example file for running several trainings in parallel with run_experiments.py
[
    'python train.py --train-config experiment_params/train_config_default.yaml --dataset-path datasets/chaotic_pendulum --epochs 5 --params optimization.all_lr=1.0e-3 optimization.end_lr_multiplier=0.025 --name decay_lr_1e-3_to_2.5e-5 -y',
    'python train.py --train-config experiment_params/train_config_default.yaml --dataset-path datasets/chaotic_pendulum --epochs 5 --params geco.initial_lagrange_multiplier=100.0 geco.lagrange_multiplier_param=1.0 geco.alpha=0.0 geco.tol=0.0 --name beta_0.01 -y',
    'python train.py --train-config experiment_params/train_config_default.yaml --dataset-path datasets/chaotic_pendulum --epochs 5 --name baseline -y',
]